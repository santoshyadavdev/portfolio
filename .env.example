# Umami Analytics
# Get your website ID from https://umami.is
PUBLIC_UMAMI_WEBSITE_ID=your-umami-website-id

# GitHub Token (used for repository data)
GITHUB_TOKEN=your-github-token

# YouTube Data API Key (optional)
# Used to display subscriber count on the contact page
# Get your API key from https://console.cloud.google.com/apis/credentials
# The site functions without this - subscriber count will simply be hidden
YOUTUBE_API_KEY=your-youtube-api-key

# Twitter/X Bearer Token (optional)
# Used to display follower count on the contact page
# Get your bearer token from https://developer.twitter.com/en/portal/dashboard
# The site functions without this - follower count will simply be hidden
TWITTER_BEARER_TOKEN=your-twitter-bearer-token
